# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Food {
  id: ID!
  color: String!
  serves: Int!
  
}
input NewFood {
  id: ID!
  color: String!
  serves: Int!

}

type Todo {
  id: ID!
  text: String!
  done: Boolean!
  user: User!
}


type User {
  id: ID!
  name: String!
}

type Query {
  todos: [Todo!]!
  pallete: [Food!]!
}

input NewTodo {
  text: String!
  userId: String!
}

type Mutation {
  createTodo(input: NewTodo!): Todo!
  createFood(input: NewFood!): Food!
}